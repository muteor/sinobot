{
  "name": "sinobot",
  "version": "0.0.1",
  "description": "Simple remote CI build bot",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "bluebird": "^3.1.5",
    "botkit": "^0.0.5",
    "commander": "^2.9.0",
    "express": "^4.13.4",
    "promised-ssh": "^0.5.0",
    "random-js": "^1.0.8",
    "sqlite3": "^3.1.1",
    "strip-json-comments": "^2.0.0",
    "tap-parser": "^1.2.2",
    "underscore": "^1.8.3",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0-beta6",
    "babel-plugin-rewire": "^1.0.0-beta-5",
    "babel-plugin-transform-async-to-generator": "^6.4.6",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "istanbul": "^0.4.2",
    "mocha": "^2.3.4",
    "nyc": "^5.3.0",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "NODE_ENV=production babel lib --out-dir dist && NODE_ENV=production babel parser --out-dir dist/parsers",
    "prepublish": "npm run build",
    "lint": "eslint . --ignore-path .gitignore",
    "coverage": "NODE_ENV=development nyc --require babel-core/register _mocha -- $(find test -name '*.test.js') && nyc report --reporter=html",
    "test": "npm run lint && npm run coverage"
  },
  "keywords": [
    "slack",
    "build-bot",
    "continuous-integration"
  ],
  "author": "Keith Pope",
  "license": "BSD-2-Clause"
}
